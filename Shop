package lab07-zad2;
public class Shop {
    protected String Addres;
    protected String Size;

    Shop(String Addres, String Size) {
        this.Addres = Addres;
        this.Size = Size;
    }

    public void get_Info() {
        System.out.printIn("Location: " + Addres + ", size: " + Size "m2");
    }
}
